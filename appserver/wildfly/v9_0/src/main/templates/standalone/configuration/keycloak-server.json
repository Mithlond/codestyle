{
  "providers": [
    "classpath:${jboss.server.config.dir}/providers/*"
  ],
  "admin": {
    "realm": "master"
  },
  "eventsStore": {
    "provider": "jpa",
    "jpa": {
      "exclude-events": [
        "REFRESH_TOKEN"
      ]
    }
  },
  "realm": {
    "provider": "jpa"
  },
  "user": {
    "provider": "jpa"
  },
  "userSessions": {
    "provider": "mem"
  },
  "realmCache": {
    "provider": "mem"
  },
  "userCache": {
    "provider": "mem",
    "mem": {
      "maxSize": 20000
    }
  },
  "timer": {
    "provider": "basic"
  },
  "theme": {
    "default": "keycloak",
    "staticMaxAge": ${keycloak.staticMaxAge},
    "cacheTemplates": ${keycloak.cacheTemplates},
    "cacheThemes": ${keycloak.cacheThemes},
    "folder": {
      "dir": "${jboss.server.config.dir}/themes"
    }
  },
  "login": {
    "provider": "freemarker"
  },
  "account": {
    "provider": "freemarker"
  },
  "email": {
    "provider": "freemarker"
  },
  "scheduled": {
    "interval": 900
  },
  "connectionsHttpClient": {
    "default": {
      "disable-trust-manager": true
    }
  },
  "connectionsJpa": {
    "default": {
      "dataSource": "java:jboss/datasources/KeycloakDS",
      "databaseSchema": "update"
    }
  }
}